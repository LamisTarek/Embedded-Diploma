#include"stdio.h"
#include <math.h>

double findsqr(double num){
	return sqrt(num);
}


int main(){
	int num;
	printf("Enter a number:");
		fflush(stdout);
		scanf("%d",&num);
		printf("Square root is: %f\n",findsqr(num));
		return 0;
}


