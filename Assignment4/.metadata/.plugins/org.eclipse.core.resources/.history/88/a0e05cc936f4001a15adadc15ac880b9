#include"stdio.h"
int check_prime(int num);
int main(){
	int num1,num2,i,flag;
	printf("Enter two numbers(intervals):");
	fflush(stdin); fflush(stdout);
	scanf("%d %d",&num1,&num2);
	printf("Prime number between %d and %d are:",num1,num2 );
	for(i=num1+1 ;i<num2;++i)
	{
		flag=check_prime(i);
		if(flag==0)
			printf("%d",i);
	}
	return 0;
}
int check_number(int num){
	int j,flag=0;
	for(j=2;j<=num/2;++j)
	{
		if(num%j==0){
			flag=1;
			break;
		}
	}
	return flag;
}
