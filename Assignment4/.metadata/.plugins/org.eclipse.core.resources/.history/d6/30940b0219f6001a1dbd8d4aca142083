//unique numbers
#include"stdio.h"
void print(int arr[],int n)
{
	int i;
	for(i=0;i<n;i++){
		printf("%d",arr[i]);
	}
}
void reverse(int arr[],int n)
{
	int temp,low,high;
	for(low=0, high=n-1; low<high ;low++,high--)
	{

		temp=arr[low];
		arr[low]=arr[high];
		arr[high]=temp;
	}
}


int main(void){
	int n;
	int arr[]={1,2,3,4,5};
	n= sizeof(arr)/sizeof(arr[0]);
	reverse(arr,n);
	print(arr,n);
	return 0;
}

















